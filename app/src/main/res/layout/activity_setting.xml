                                android:layout_gravity="bottom"
                                android:shadowColor="@color/black"
                                android:shadowDx="1"
                                android:shadowDy="2"
                                android:shadowRadius="1"
                                android:layout_marginStart="@dimen/_10sdp"
                                android:layout_marginBottom="@dimen/_10sdp"/>

                        </FrameLayout>
