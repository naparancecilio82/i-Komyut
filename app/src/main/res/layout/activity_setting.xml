                        </FrameLayout>
            -->
                    </LinearLayout>
                </HorizontalScrollView>

                <TextView
                    android:id="@+id/saveLocTxt"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Saved Location"
                    android:textColor="@color/blue"
                    android:fontFamily="@font/poppins_bold"
                    android:textSize="16sp"
                    android:layout_marginTop="@dimen/_20sdp" />
                <LinearLayout
                    android:id="@+id/savedLocationBtn"
                    android:layout_width="match_parent"
                    android:layout_gravity="center_horizontal"
                    android:background="@drawable/fragment_setting_container"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="@dimen/_20sdp"
                            android:layout_height="@dimen/_20sdp"
                            android:src="@drawable/ic_location"
                            android:layout_gravity="center"
                            android:layout_marginStart="@dimen/_20sdp"/>
                        <TextView

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Search history"
                            android:textColor="@color/blue"
                            android:fontFamily="@font/poppins_semi_bold"
                            android:textSize="14sp"
                            android:layout_marginTop="@dimen/_10sdp"
                            android:layout_marginBottom="@dimen/_10sdp"
                            android:layout_marginStart="@dimen/_10sdp"/>
                    </LinearLayout>
                </LinearLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Modes of Transit"
                    android:textColor="@color/blue"
                    android:fontFamily="@font/poppins_bold"
                    android:textSize="16sp"
                    android:layout_marginTop="@dimen/_20sdp" />
                <LinearLayout
                    android:id="@+id/transitBtn"
                    android:layout_width="match_parent"
                    android:layout_gravity="center_horizontal"
                    android:background="@drawable/fragment_setting_container"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="@dimen/_20sdp"
                            android:layout_height="@dimen/_20sdp"
                            android:src="@drawable/ic_tric"
                            android:layout_gravity="center"
                            android:layout_marginStart="@dimen/_20sdp"/>
                        <TextView

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Tricycle and Jeepney Routes"
                            android:textColor="@color/blue"
                            android:fontFamily="@font/poppins_semi_bold"
                            android:textSize="14sp"
                            android:layout_marginTop="@dimen/_10sdp"
                            android:layout_marginBottom="@dimen/_10sdp"
                            android:layout_marginStart="@dimen/_10sdp"/>
                    </LinearLayout>
                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Get Help"
                    android:textColor="@color/blue"
                    android:fontFamily="@font/poppins_bold"
                    android:textSize="16sp"
                    android:layout_marginTop="@dimen/_10sdp"/>

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_gravity="center_horizontal"
                    android:background="@drawable/fragment_setting_container"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/FAQs"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="@dimen/_20sdp"
                            android:layout_height="@dimen/_20sdp"
                            android:src="@drawable/ic_faqs"
                            android:layout_gravity="center"
                            android:layout_marginStart="@dimen/_20sdp"/>
                        <TextView

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="FAQ"
                            android:textColor="@color/blue"
                            android:fontFamily="@font/poppins_semi_bold"
                            android:textSize="14sp"
                            android:layout_marginTop="@dimen/_10sdp"
                            android:layout_marginBottom="@dimen/_10sdp"
                            android:layout_marginStart="@dimen/_10sdp"/>
                    </LinearLayout>
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"
                        android:paddingStart="@dimen/_10sdp"
                        android:paddingEnd="@dimen/_10sdp"
                        android:src="@drawable/fragment_transit_underline" />

                    <LinearLayout
                        android:id="@+id/contactUs"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="@dimen/_20sdp"
                            android:layout_height="@dimen/_20sdp"
                            android:src="@drawable/ic_contact_us"
                            android:layout_gravity="center"
                            android:layout_marginStart="@dimen/_20sdp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Contact Us"
                            android:textColor="@color/blue"
                            android:fontFamily="@font/poppins_semi_bold"
                            android:textSize="14sp"
                            android:layout_marginTop="@dimen/_10sdp"
                            android:layout_marginBottom="@dimen/_10sdp"
                            android:layout_marginStart="@dimen/_10sdp"/>

                    </LinearLayout>
                </LinearLayout>

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Suggestion and Feedbacks"
                    android:textColor="@color/blue"
                    android:fontFamily="@font/poppins_bold"
                    android:layout_marginTop="@dimen/_10sdp"
                    android:textSize="16sp"
                    />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_gravity="center_horizontal"
                    android:background="@drawable/fragment_setting_container"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/suggestionBtn"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <ImageView
                            android:layout_width="@dimen/_20sdp"
                            android:layout_height="@dimen/_20sdp"
                            android:src="@drawable/ic_email"
                            android:layout_gravity="center"
                            android:layout_marginStart="@dimen/_20sdp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Suggestion and Feedbacks"
                            android:textColor="@color/blue"
                            android:fontFamily="@font/poppins_semi_bold"
                            android:textSize="14sp"
                            android:layout_marginTop="@dimen/_10sdp"
                            android:layout_marginBottom="@dimen/_10sdp"
                            android:layout_marginStart="@dimen/_10sdp"/>

                    </LinearLayout>
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"
                        android:paddingStart="@dimen/_10sdp"
                        android:paddingEnd="@dimen/_10sdp"
                        android:src="@drawable/fragment_transit_underline" />

                    <LinearLayout
                        android:id="@+id/rateBtn"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="@dimen/_20sdp"
                            android:layout_height="@dimen/_20sdp"
                            android:src="@drawable/ic_rate"
                            android:layout_gravity="center"
                            android:layout_marginStart="@dimen/_20sdp"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Rate Us"
                            android:textColor="@color/blue"
                            android:fontFamily="@font/poppins_semi_bold"
                            android:textSize="14sp"
                            android:layout_marginTop="@dimen/_10sdp"
                            android:layout_marginBottom="@dimen/_10sdp"
                            android:layout_marginStart="@dimen/_10sdp"/>

                    </LinearLayout>
                </LinearLayout>
                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="Terms of Service"
                    android:textColor="@color/blue"
                    android:fontFamily="@font/poppins_bold"
                    android:textSize="16sp"
                    android:layout_marginTop="@dimen/_15sdp"
                    />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_gravity="center_horizontal"
                    android:background="@drawable/fragment_setting_container"
                    android:layout_height="wrap_content"
                    android:layout_marginBottom="@dimen/_15sdp"
                    android:orientation="vertical">

                    <LinearLayout
                        android:id="@+id/TCBtn"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="@dimen/_20sdp"
                            android:layout_height="@dimen/_20sdp"
                            android:src="@drawable/ic_privacy"
                            android:layout_gravity="center"
                            android:layout_marginStart="@dimen/_20sdp"/>

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Terms and Conditions"
                            android:textColor="@color/blue"
                            android:fontFamily="@font/poppins_semi_bold"
                            android:textSize="14sp"
                            android:layout_marginTop="@dimen/_10sdp"
                            android:layout_marginBottom="@dimen/_10sdp"
                            android:layout_marginStart="@dimen/_10sdp"/>

                    </LinearLayout>
                    <ImageView
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/_1sdp"
                        android:paddingStart="@dimen/_10sdp"
                        android:paddingEnd="@dimen/_10sdp"
                        android:src="@drawable/fragment_transit_underline" />

                    <LinearLayout
                        android:id="@+id/PPBtn"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">
                        <ImageView
                            android:layout_width="@dimen/_20sdp"
                            android:layout_height="@dimen/_20sdp"
                            android:src="@drawable/ic_terms"
                            android:layout_gravity="center"
                            android:layout_marginStart="@dimen/_20sdp"/>
                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
