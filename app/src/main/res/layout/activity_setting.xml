                                android:layout_height="@dimen/_75sdp"
                                android:layout_marginBottom="@dimen/_minus15sdp"
                                android:layout_marginEnd="@dimen/_2sdp"
                                android:src="@drawable/logo_only"
                                android:layout_gravity="bottom|end"/>
